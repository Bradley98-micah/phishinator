
no='\033[1;33m'
update='\033[1;32m'
nc='\033[0m'
rm -rf update
echo ${no}Checking for Update.. Please wait..
git clone https://github.com/Philiphackerzx/update --quiet
cd update
version=`cat version.io | grep "phi" | cut -c6-`
clear

if [ $version -eq 2 ]
then
echo ${update}   Good News.. Update available !!
echo ${nc}   Press any key to update now..
read pause
clear
echo ${update} Updating... Please wait...
echo ${nc} Do not press anything...
cd /data/data/com.termux/files/home/
rm -rf phishinator
git clone https://github.com/Philiphackerzx/phishinator.git --quiet
cd phishinator
sh start
else
clear 
echo ${no}No update available.. ${update}You are up-to-date âœ“ ${nc}
echo Press any key to continue..
read pause
cd /data/data/com.termux/files/home/phishinator/
sh start
fi
